{
  "name": "fullstackdotdev-app",
  "version": "0.1.0",
  "bin": {
    "fullstackdotdev-app": "bin/fullstackdotdev-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'lib/**/*.ts' --write"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.83.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "aws-cdk": "1.83.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "jest": "^26.4.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.83.0",
    "@aws-cdk/aws-apigatewayv2": "^1.83.0",
    "@aws-cdk/aws-iam": "^1.83.0",
    "@aws-cdk/aws-lambda": "^1.83.0",
    "@aws-cdk/core": "1.83.0",
    "source-map-support": "^0.5.16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npx eslint . --ext .ts --fix"
    ]
  }
}
